	<munit:config name="teams-test.xml" />
	<munit:test name="teams-test-teams_getTeamByTag_subFlowTest" description="Test" doc:id="40af3276-ff7e-4d71-b636-fa652f5874a0">
		<munit:behavior>
			<munit-tools:mock-when doc:name="Mock when" doc:id="7875146d-99a8-4f18-a9ee-9ea7f3f4f3d9" processor="db:select">
				<munit-tools:with-attributes>
					<munit-tools:with-attribute attributeName="doc:name" whereValue="#['Select team by tag']" />
				</munit-tools:with-attributes>
				<munit-tools:then-return>
					<munit-tools:payload value="#[readUrl('classpath://cosmonauts.dwl')]" mediaType="application/java" encoding="UTF-8" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution>
			<flow-ref doc:name="Flow-ref to teams_getTeamByTag_subFlow" doc:id="12b61d9a-02aa-43e7-95e8-5aeb3943bd52" name="teams_getTeamByTag_flow" />
		</munit:execution>
		<munit:validation>
			<ee:transform doc:name="Transform Message" doc:id="9ceb96a1-febe-427d-acdf-721af73ddeff" >
				<ee:message >
				</ee:message>
				<ee:variables >
					<ee:set-variable variableName="expected" resource="cosmonauts.dwl"/>
				</ee:variables>
			</ee:transform>
			<munit-tools:assert-that doc:name="Assert that" doc:id="6b7ba554-83e0-49b0-936b-c8bc1e547604" expression="#[payload]" is="#[MunitTools::equalTo(vars.expected)]" />
		</munit:validation>
	</munit:test>
