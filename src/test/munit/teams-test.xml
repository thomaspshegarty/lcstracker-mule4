	<munit:test name="teams-test-teams_putTeam_subFlowTest" description="Test" doc:id="e3965dbb-55c8-459f-9079-3733fc485dd9" >
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="ce7e8b66-6083-4638-9769-95ad5e867e01" processor="db:update">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute attributeName="doc:name" whereValue="#['Update the team of given tag']" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value="#[readUrl('classpath://cosmonauts.dwl')]" mediaType="application/java" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<ee:transform doc:name="Transform Message" doc:id="b88e23ee-f4d0-43a5-88c7-ebd2a67b5108" >
				<ee:message >
					<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
	'tag':'csm'
}]]></ee:set-payload>
				</ee:message>
			</ee:transform>
			<flow-ref doc:name="Flow-ref to teams_putTeam_subFlow" doc:id="7372f269-3363-4502-b6bc-0776d5671a5b" name="teams_patchTeam_flow"/>
		</munit:execution>
