	<munit:test name="teams-test-teams_getTeamsByRegion_subFlowTest" description="Test" doc:id="235a31b4-32b7-4cbe-8a1e-f7ce2e9464a4" >
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="1bce02ef-36aa-448e-8da4-d08585fccf89" processor="db:select">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute attributeName="doc:name" whereValue="#['Select all teams from region']" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value="#[[{'team':'cosmonauts','tag':'csm','region':'na','wins':10,'losses':0}]]" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="Flow-ref to teams_getTeamsByRegion_subFlow" doc:id="976d8e2f-673b-4319-b0f3-8e66dbdfe3e4" name="teams_getTeamsByRegion_flow"/>
